    </ul>
  </nav>
  <body>
    
    <div style="font-size:60px">
        <h2>Favourite Songs</h2>

        <% if (songs.length > 0){ %>
            <% songs.forEach(song => { %>
               <h3><%= song.title %></h3>
               <p><%= song.artist %></p>
               <p><%= song.link %></p>
        <% }) %>
        <% } %>
    
    </div>
  </body>
</html>
